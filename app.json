{
  "name": "Drink Up, Buttercup",
  "description": "Drink more beer",
  "repository": "https://github.com/coreyk/drinkupbuttercup",
  "logo": "http://manaron.s3.amazonaws.com/kais/drinkup.png",
  "success_url": "/",
  "env": {
    "HEROKU_APP_NAME": {
      "required": true
    },
    "APP_COMMAND_TOKEN": {
      "description": "Slash command token, for the app command endpoint",
